{
  "name": "grouparty",
  "version": "3.0.0",
  "description": "WDI_25 Project 3 - Grouparty",
  "main": "server.js",
  "engines": {
    "node": "10.21.x",
    "npm": "6.14.x"
  },
  "scripts": {
    "postinstall": "bower install && gulp deploy",
    "start": "node server.js"
  },
  "author": "Raiden Dilan <raiden18@me.com> (https://github.com/raidendilan)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raidendilan/grouparty/issues"
  },
  "homepage": "https://github.com/raidendilan/grouparty#readme",
  "gulp": {
    "srcDir": "src/",
    "destDir": "public/",
    "src": {
      "js": "src/js/",
      "scss": "src/scss/",
      "fonts": "src/fonts/",
      "images": "src/images/"
    },
    "dest": {
      "js": "public/js/",
      "css": "public/css/",
      "fonts": "public/fonts/",
      "images": "public/images/"
    },
    "main": {
      "js": "app.js",
      "index": "index.html",
      "scss": "style.scss",
      "server": "server.js",
      "gulpfile": "gulpfile.js",
      "packageJson": "package.json"
    },
    "vendor": {
      "js": "vendor.js",
      "css": "vendor.css"
    },
    "output": {
      "js": "app.js",
      "css": "style.css"
    },
    "selectors": {
      "all": "*",
      "js": "**/*.js",
      "css": "**/*.css",
      "scss": "**/*.scss",
      "html": "**/*.html",
      "images": "**/*.{png,jpg,jpeg,gif,ico,svg}",
      "fonts": "**/*.{eot,svg,ttf,woff,woff2}"
    }
  },
  "dependencies": {
    "angular-chart.js": "^1.1.1",
    "angular-material": "^1.1.21",
    "aws-sdk": "^2.656.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "chart.js": "^2.9.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "ng2-charts": "^2.3.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "bower": "^1.8.8",
    "browser-sync": "^2.26.7",
    "eslint": "^6.8.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "event-stream": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlhint": "^3.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-jshint": "^2.1.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-notify": "^3.2.0",
    "gulp-order": "^1.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-strip-css-comments": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "htmlhint-stylish": "^1.0.3",
    "jshint": "^2.11.0",
    "jshint-stylish": "^2.2.1",
    "main-bower-files": "^2.13.3",
    "require-dir": "^1.2.0"
  }
}